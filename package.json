{
  "name": "vueApp",
  "main": "src/app.js",
  "aliasify": {
    "aliases": {
      "$vue": "vue/dist/vue.js"
    }
  },
  "devDependencies": {
    "aliasify": "^2.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "jquery": "^3.1.1",
    "lodash": "^4.16.4",
    "uglifyjs": "^2.4.10",
    "vue": "^2.0.3"
  },
  "scripts": {
    "build": "browserify src/app.js -o > dist/vue-app.js -t [ babelify --presets [ es2015 ] ]",
    "watch": "watchify src/app.js -o dist/vue-app.js"
  }
}
