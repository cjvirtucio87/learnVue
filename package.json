{
  "name": "vueApp",
  "main": "src/app.js",
  "aliasify": {
    "aliases": {
      "$vue": "vue/dist/vue.js"
    }
  },
  "devDependencies": {
    "aliasify": "^2.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "jquery": "^3.1.1",
    "uglify": "^0.1.5",
    "vue": "^2.0.3",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "watch": "watchify src/main.js -o dist/vue-app.js",
    "build": "browserify src/main.js | uglifyjs -mc warnings=false > dist/vue-app.js"
  }
}
